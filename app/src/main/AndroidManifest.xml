<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chens.artists">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.WebsiteActivity" />
        <activity android:name=".ui.WallPaperActivity" />
        <activity android:name=".ui.SongActivity" />
        <activity android:name=".ui.MailListActivity" />
        <activity android:name=".ui.VideoActivity" />
        <activity android:name=".ui.FacebookActivity"></activity>
    </application>

</manifest>
